    <script>
        function togglePhase(phaseElement) {
            const weeksContainer = phaseElement.querySelector('.weeks-container');
            if (weeksContainer) {
                if (weeksContainer.classList.contains('active')) {
                    weeksContainer.classList.remove('active');
                    phaseElement.classList.remove('active');
                    phaseElement.style.background = 'rgba(22, 10, 15, 0.8)';
                } else {
                    weeksContainer.classList.add('active');
                    phaseElement.classList.add('active');
                    phaseElement.style.background = 'rgba(35, 20, 25, 0.9)';
                }
            }
        }

        function toggleDetails(event, button) {
            event.stopPropagation();
            const weekCard = button.closest('.week-card');
            const details = weekCard.querySelector('.week-details-expanded');
            
            if (details.style.display === 'none' || details.style.display === '') {
                details.style.display = 'block';
                button.textContent = 'Hide Details';
            } else {
                details.style.display = 'none';
                button.textContent = 'Show Details';
            }
        }

        // Initialize with first phase open
        document.addEventListener('DOMContentLoaded', function() {
            const firstPhase = document.querySelector('.phase-1');
            if (firstPhase) {
                const weeksContainer = firstPhase.querySelector('.weeks-container');
                if (weeksContainer) {
                    weeksContainer.classList.add('active');
                    firstPhase.classList.add('active');
                    firstPhase.style.background = 'rgba(35, 20, 25, 0.9)';
                }
            }
        });
    </script>
</body>
</html>
